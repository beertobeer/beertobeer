            <script src="{{ asset('bundles/beertobeercore/js/views/business.js') }}"></script>
            <script src="{{ asset('bundles/beertobeercore/js/views/businesses.js') }}"></script>
            <script src="https://maps.googleapis.com/maps/api/js?key=AIzaSyA8f7_iudMUZO6Mrbf8Rywz2QuIm3kgUgM&amp;sensor=false&amp;libraries=places"></script>
            <script src="{{ asset('bundles/beertobeercore/js/libs/jquery.geocomplete.min.js') }}"></script>
            <script src="{{ asset('bundles/beertobeercore/js/app.js') }}"></script>
            <script src="{{ asset('bundles/beertobeercore/js/routers/router.js') }}"></script>
