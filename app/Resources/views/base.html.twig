        <meta name="msapplication-config" content="{{ asset("favicons/browserconfig.xml") }}">
        {% block stylesheets %}
            {% stylesheets 'bundles/beertobeercore/css/*' filter='cssrewrite' %}
                <link rel="stylesheet" href="{{ asset_url }}" />
            {% endstylesheets %}
        {% endblock %}
    </head>
