                '@BeerToBeerCoreBundle/Resources/public/js/models/*'
                '@BeerToBeerCoreBundle/Resources/public/js/collections/*'
                '@BeerToBeerCoreBundle/Resources/public/js/routers/*'
                '@BeerToBeerCoreBundle/Resources/public/js/views/*'
                '@BeerToBeerCoreBundle/Resources/public/js/*'
                 %}
                <script src="{{ asset_url }}"></script>
            {% endjavascripts %}#}
            <script src="{{ asset('bundles/beertobeercore/js/libs/jquery-2.1.1.min.js') }}"></script>
            <script src="{{ asset('bundles/beertobeercore/js/libs/underscore.js') }}"></script>
            <script src="{{ asset('bundles/beertobeercore/js/libs/backbone.js') }}"></script>
            <script src="{{ asset('bundles/beertobeercore/js/libs/jquery-ui.min.js') }}"></script>
