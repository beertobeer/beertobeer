<div class="row oneBeerBusiness <% if (typeof volume == "undefined") { %>undefined<% } %>" id="<%= id %>">
	<div class="col-xs-3 volume">
	<% if (typeof volume != "undefined") { %>
		<%= volume %> cl
	<% } else { %>
		<input type="number" step="1" value="" min="0" class="form-control inputVolume" id="<%= id %>"> cl
	<% } %>
	</div>
	<div class="col-xs-3 prix">
		<% var prixNormal = prixNormal || ""; %>
		<input type="number" step="0.1" value="<%= prixNormal %>" min="0" class="form-control inputNormal" placeholder="-" id="<%= id %>">
	</div>
	<div class="col-xs-3 prix">
		<% var prixHappyHour = prixHappyHour || ""; %>
		<input type="number" step="0.1" value="<%= prixHappyHour %>" min="0" class="form-control inputHappyHour" placeholder="-" id="<%= id %>">
	</div>
	<div class="col-xs-3 remove">
		<a href="#">
			<img src="{{ asset("bundles/beertobeercore/images/remove.png") }}" alt="Supprimer" id="<%= id %>">
		</a>
	</div>
</div>